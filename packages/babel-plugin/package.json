    "@harmony/tsconfig": "workspace:^",
    "@testing-library/react": "^16.2.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__traverse": "^7.20.6",
    "@types/node": "^22.10.7",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "eslintConfig": {
