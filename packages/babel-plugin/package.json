{
  "name": "babel-plugin-harmony",
  "version": "0.2.7",
  "description": "",
  "author": "",
  "license": "ISC",
  "keywords": [
    "babel-plugin"
  ],
  "main": "dist/bundle.js",
  "types": "dist/esm",
  "scripts": {
    "build": "NODE_ENV=production webpack --mode=production",
    "test": "vitest run",
