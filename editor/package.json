{
    "name": "harmony-ai-editor",
    "description": "Front-end overlay ontop of your webapp",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/esm/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "npm run build:esm && npm run build:cjs",
        "build:esm": "tsc && tsc-alias",
        "build:cjs": "tsc --module commonjs --outDir dist/cjs && tsc-alias"
    },
    "dependencies": {
        "dayjs": "^1.11.10",
        "hotkeys-js": "^3.13.2",
        "react": "^18",
        "react-reconciler": "^0.29.0",
        "tsc-alias": "^1.8.8",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "@types/react-reconciler": "^0.28.8",
        "typescript": "^5"
    },
    "version": "1.0.10"
}
