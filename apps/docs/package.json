{
  "name": "tailwindui-syntax",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "withenv": "dotenv -e ../../.env --",
    "dev": "pnpm withenv next dev",
    "build": "pnpm withenv next build",
    "typecheck": "tsc --noEmit",
    "start": "pnpm withenv next start",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\" && eslint --fix ."
  },
  "browserslist": "defaults, not ie <= 11",
  "dependencies": {
    "@algolia/autocomplete-core": "^1.17.9",
    "@headlessui/react": "^2.2.0",
    "@markdoc/markdoc": "^0.4.0",
    "@markdoc/next.js": "^0.3.7",
    "@sindresorhus/slugify": "^2.2.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.10.7",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@types/react-highlight-words": "^0.20.0",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "dotenv-cli": "^8.0.0",
    "fast-glob": "^3.3.3",
    "flexsearch": "^0.7.43",
    "js-yaml": "^4.1.0",
    "next": "^15.1.5",
    "next-themes": "^0.4.4",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-highlight-words": "^0.21.0",
    "simple-functional-loader": "^1.2.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@harmony/eslint-config-custom": "workspace:*",
    "@harmony/prettier-config": "workspace:^",
    "@harmony/tailwind-config": "workspace:^",
    "eslint": "^9.18.0",
    "eslint-config-next": "^15.1.5",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "sharp": "0.33.5"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@harmony/eslint-config-custom/library"
    ]
  },
  "prettier": "@harmony/prettier-config"
}
