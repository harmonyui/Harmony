{
  "manifest_version": 3,
  "name": "Harmony UI Editor",
  "version": "1.0",
  "permissions": ["activeTab", "storage", "contextMenus", "tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    },
    {
      "js": ["dist/bundle.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["dist/bundle.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
